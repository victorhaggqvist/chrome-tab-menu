{
  "manifest_version": 2,

  "name": "Tab Menu",
  "description": "Have tabs in a handy searchable list",
  "version": "0.1",

  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_icon": "assets/icon19.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "assets/icon16.png",
    "48": "assets/icon48.png",
    "128": "assets/icon128.png"
  }
}
