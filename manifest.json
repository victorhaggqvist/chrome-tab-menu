{
  "manifest_version": 2,

  "name": "Tab Menu",
  "description": "Have tabs in a handy searchable list",
  "version": "0.4.1",

  "permissions": [
    "tabs"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon.png",
    "19": "icon.png",
    "38": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
